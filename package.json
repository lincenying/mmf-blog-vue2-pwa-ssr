    "vue": "^2.7.14",
    "vue-content-loader": "0.2.3",
    "vue-meta": "2.4.0",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "^0.6.0",
